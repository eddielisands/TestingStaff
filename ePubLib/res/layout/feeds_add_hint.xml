<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:background="#5888" android:id="@+id/feeds_hint_relative_layout">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="35dip" android:layout_height="wrap_content" android:layout_alignParentLeft="true">
        <ImageView android:layout_height="wrap_content" android:layout_marginTop="35dip" android:id="@+id/imageView1" android:layout_width="wrap_content" android:layout_marginLeft="20dip" android:src="@drawable/feeds_hint_symbol"></ImageView>
    </RelativeLayout>
    <RelativeLayout android:layout_height="wrap_content" android:id="@+id/relativeLayout2" android:layout_width="match_parent" android:layout_toRightOf="@+id/relativeLayout1" android:layout_alignParentRight="true" android:layout_marginTop="24dip" android:layout_marginLeft="15dip" android:paddingBottom="30dip">
        <LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/linearLayout1" android:layout_width="match_parent">
            <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/textView1" android:textSize="32sp" android:text="@string/feed_hint_title"></TextView>
            <TextView android:id="@+id/textView2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/feed_hint_subtitle"></TextView>
        </LinearLayout>
    </RelativeLayout>
    
</RelativeLayout>
